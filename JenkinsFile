pipeline {
  agent any
  tools {
    maven 'maven'
  }
  stages {
    stage ('scm') {
      steps {
        echo "clonning the repo"
        git branch: 'main', url: 'https://github.com/Arsenet7/carles-project'
      }
    }
    stage ('build&test') {
      steps {
        sh ''' 
        java --version
        mvn clean package
        '''
        
      }
    }
  }
}
